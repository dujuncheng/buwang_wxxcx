.tuan-contaner {
  width: 100%;
  height: 88rpx;
  background-color: white;
  display: flex;
  align-items: center;
  padding-left: 24rpx;
  padding-right: 24rpx;
  box-sizing: border-box;
}

.text-container {
	flex: 1;
	display: flex;
	align-items: center;
	ont-family: PingFangSC-Regular;
	font-size: 29rpx;
	color: #333333;
	letter-spacing: 0;
	display: flex;
	text-align: left;
	align-content: center;
}

.avater-container {
	flex: 1;
	display: flex;
	align-items: center;
	justify-content: flex-end;
}

.avater-img {
	width: 48rpx;
	height: 48rpx;
	border-radius: 50%;
	margin-right: 12rpx;
}
